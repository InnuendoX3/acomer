<h3>All Restaurants</h3>

<% for (const iterator of data) { %>
   <div class="card col-md-3">
      <div class="card-body">
         <div>
            <h4 class="card-title">
               <a href="/restaurants/<%= iterator.restaurantID %>">
                  <%= iterator.name %>
               </a>
            </h4>
         </div>
         <div>
            <%= iterator.type %>
         </div>
         <div>
            <em><%= iterator.city %></em>
         </div>
         <div>
            <em><%= iterator.address %></em>
         </div>
         <div>
            Rating: <strong> <%= iterator.rating %> </strong>
         </div>
      </div>
   </div>
<% } %>
