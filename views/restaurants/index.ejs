<h3>All Restaurants</h3>

<% for (const iterator of data) { %>
   <div>
      <div>
         <h4>
            <a href="/restaurants/<%= iterator.restaurantID %>">
               <%= iterator.name %>
            </a>
         </h4>
      </div>
      <div>
         <%= iterator.type %>
      </div>
      <div>
         <%= iterator.city %>
      </div>
      <div>
         <%= iterator.address %>
      </div>
      <div>
         <%= iterator.rating %>
      </div>

      ----
   </div>
<% } %>
